    "default_flds = {'font': 'Arial',\n",
    "                'media': [],\n",
    "                'name': 'name',\n",
    "                'ord': 1,\n",
    "                'rtl': False,\n",
    "                'size': 20,\n",
    "                'sticky': False}\n",
    "\n",
    "def gen_flds(cnames):\n",
    "    ecnames = enumerate(cnames)\n",
    "    def gen_fld(ordid, name, custom):\n",
    "        for key in custom.keys() - default_flds.keys():\n",
    "            del custom[key]\n",
    "        \n",
    "        fld = default_flds.copy()\n",
    "        fld['ord'] = ordid\n",
    "        fld['name'] = name\n",
    "        return fld\n"
   ]
